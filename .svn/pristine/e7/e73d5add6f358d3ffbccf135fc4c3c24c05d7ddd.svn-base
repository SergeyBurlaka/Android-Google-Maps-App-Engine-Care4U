<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.quest.uni2b.appengine.gsm.demo.usertouserdemo2svn020316.Manager.ManagerCabinetActivity"
    tools:showIn="@layout/activity_manager_cabinet">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/EmployeeReqTitle"
        android:id="@+id/EmployeeReqButton"
        android:layout_alignParentTop="true"

        android:layout_toStartOf="@+id/confirmEmployeesBtn" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/confirmEBTitle"
        android:id="@+id/confirmEmployeesBtn"
        android:enabled="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <ListView
        android:id="@+id/lvMain"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_below="@+id/EmployeeReqButton"
        android:layout_alignEnd="@+id/confirmEmployeesBtn">
    </ListView>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/checkTitle"
        android:id="@+id/checkButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="50dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="10dp"
        android:layout_height="20dp"

        android:id="@+id/managerMapButton"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:padding="30dp"

        android:drawableLeft="@drawable/markermap"
        android:layout_toStartOf="@+id/lvMain"
        android:layout_above="@+id/lvMain"
        android:layout_alignEnd="@+id/checkButton" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/MyFriendiTitle"
        android:id="@+id/MyFrndbutton"
        android:layout_marginTop="56dp"
        android:layout_alignTop="@+id/lvMain"
        android:layout_alignParentStart="true" />


</RelativeLayout>
